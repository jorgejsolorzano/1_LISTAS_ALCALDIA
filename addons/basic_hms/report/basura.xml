














<record id="action_report_action_name" model="ir.actions.report">
    <field name="name">Name</field>
    <field name="model">action_name</field>
    <field name="report_type">qweb-pdf</field>
    <field name="report_name">report.name</field>
    <field name="report_file">report.name</field>
    <field name="print_report_name">'%s' % object.name</field>
    <field name="binding_model_id" ref="model_action_name"/>
    <field name="binding_type">report</field>
</record>







<template id="report_template_name">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-call="web.external_layout">
                <div class="page">
                    
                </div>
            </t>
        </t>
    </t>
</template>